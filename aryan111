        $("#liacd").html('Outstation');
        $("#ctl00_SearchControl_desktop_lioutstation").addClass("active");
        $("#ctl00_SearchControl_desktop_liwithin").removeClass("active");
        $("#ctl00_SearchControl_desktop_liairport").removeClass("active");


        $("#ctl00_SearchControl_desktop_lioneway").removeClass("active");
    }

    function set_4() {
        document.getElementById("ctl00_SearchControl_desktop_hd").value = "CD";
        document.getElementById("ctl00_SearchControl_desktop_hdmulticity").value = "";
