<script lang="javascript" type="text/javascript">

    function showOnKeyPress() {
        if (document.getElementById("ctl00_SearchControl_desktop_hd").value != "CD") {
            document.getElementById('div_AddPopup').style.display = 'block';
        }
    }

    function hideOnKeyPress() {
        if (document.getElementById("ctl00_SearchControl_desktop_hd").value != "CD") {
            document.getElementById('div_AddPopup').style.display = 'none';
        }
    }
</script>
<script type="text/javascript">
    function fdrpchange() {

        $("#txtpickupaddress").val(""); $("#txtdropaddress").val("");
    }
    function onlyIntNumbers(evt) {

        var charCode = (evt.which) ? evt.which : evt.keyCode;

        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }

        return true;

    }
    function set_isd_cd() {
        $("#liisd_cd").addClass("active");
        $("#lisd").removeClass("active");
        $("#licd").removeClass("active");
    }

    function GetSelectedTextValue(ddlFruits) {
        var selectedValue = ddlFruits.value;
        $("#DrpCntryCode").val('+' + selectedValue);
        $("#socialDrpCntryCode").val('+' + selectedValue);

    }

    function set_sd() {

        document.getElementById("ctl00_SearchControl_desktop_hd").value = "SD";
        if (document.getElementById("ctl00_SearchControl_desktop_hdtype").value != "Flex") { document.getElementById("ctl00_SearchControl_desktop_hdtype").value = ""; }
        $("#lisd").addClass("active");
        $("#licd").removeClass("active");
        $("#offdate").show();
        $("#tocity").hide();
        $("#othercity").hide();
        $("#drpCar").show();
        $(".new-condition").show();
        $('#lpickup').html("Delivery Address");
        $('#txtPickUp').attr('placeholder', 'Delivery Address');
        document.getElementById("ctl00_SearchControl_desktop_ReqPickUp").innerHTML = "Enter Delivery Address !";
        $("#lishuttle").removeClass("active");
        $("#resdiv").show();
        $(".multiCityFormDetail").hide();
        $("#outmulticity").hide();
        $("#drp_type").hide();
    }

    function set_cd() {
        set_multiform();

        document.getElementById("ctl00_SearchControl_desktop_hd").value = "CD";
        document.getElementById("ctl00_SearchControl_desktop_hdtype").value = "";
        $("#licd").addClass("active");
        $("#lisd").removeClass("active");
        $("#lishuttle").removeClass("active");
        // $("#offdate").hide();
        // $("#othercity").hide();
        $(".new-condition").hide();
        $("#drp_type").show();
        $('#lpickup').html("Pickup Address");
        $('#txtPickUp').attr('placeholder', 'Pickup Address');
        document.getElementById("ctl00_SearchControl_desktop_ReqPickUp").innerHTML = "Enter Pickup Address !";

    }
    function set_cd_shuttle() {
        set_multiform();


        document.getElementById("ctl00_SearchControl_desktop_hd").value = "CD";
        document.getElementById("ctl00_SearchControl_desktop_hdtype").value = "shuttle";

        $("#lishuttle").addClass("active");
        $("#lisd").removeClass("active");
        $("#licd").removeClass("active");
        $("#pickupaddress").show();
        $("#dropupaddress").show();
        $(".new-condition").hide();
        $("#drp_type").hide();
        $("#drpCar").hide();
        $("#sdpickup").hide();



    }

    function set_multiform() {
        if (document.getElementById("ctl00_SearchControl_desktop_hdmulticity").value != "") {
            $("#resdiv").hide();
            $("#outmulticity").show();
        } else {
            $("#resdiv").show();
            $("#outmulticity").hide();
        }
        if ($('#f10').css('display') == 'block') {
            $("#shw").hide();
        }
    }

    function set_1() {
        document.getElementById("ctl00_SearchControl_desktop_hd").value = "CD";
        document.getElementById("ctl00_SearchControl_desktop_hdmulticity").value = "";
        set_multiform();
        $("#drpCar").hide();
        $(".multiCityFormDetail").hide();
        $(".dropdown-menu").hide();
        document.getElementById("ctl00_SearchControl_desktop_hdtype").value = "airport";
        $("#offdate").hide();
        $("#tocity").hide();
        $("#drp_type").show();
        $("#liacd").html('Airport & Railway Transfers');
        //New
        $("#pickupaddress").show();
        $("#dropupaddress").show();
        $("#sdpickup").attr("Style", "display:none");
        $("#ctl00_SearchControl_desktop_liairport").addClass("active");
        $("#ctl00_SearchControl_desktop_liwithin").removeClass("active");
        $("#ctl00_SearchControl_desktop_lioutstation").removeClass("active");
        $("#ctl00_SearchControl_desktop_lioneway").removeClass("active");

    }

    function set_2() {
        document.getElementById("ctl00_SearchControl_desktop_hd").value = "CD";
        document.getElementById("ctl00_SearchControl_desktop_hdmulticity").value = "";
        set_multiform();
        $("#drpCar").hide();
        $(".multiCityFormDetail").hide();
        $(".dropdown-menu").hide();
        document.getElementById("ctl00_SearchControl_desktop_hdtype").value = "local";
        $("#offdate").show();
        $("#tocity").hide();
        $("#drp_type").hide();
        $("#liacd").html('Within City');
        $("#ctl00_SearchControl_desktop_liwithin").addClass("active");
        $("#ctl00_SearchControl_desktop_liairport").removeClass("active");

        $("#ctl00_SearchControl_desktop_lioutstation").removeClass("active");
        $("#ctl00_SearchControl_desktop_lioneway").removeClass("active");
    }

    function set_3() {
        document.getElementById("ctl00_SearchControl_desktop_hd").value = "CD";
        $("#outmulticity").show();
        set_multiform();
        $("#drpCar").hide();
        $(".dropdown-menu").hide();
        document.getElementById("ctl00_SearchControl_desktop_hdtype").value = "outstation";
        $("#offdate").show();
