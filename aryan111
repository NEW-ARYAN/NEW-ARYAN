                $("#hdto9").val(i.item.val);
                $("#txtfrom10").val(i.item.label);
                $("#txtFrom10").focus();
            },
            minLength: 1
        });
        $("#txtto10").autocomplete({
            source: function (request, response) {
                document.getElementById("hdto10").value = "";
                var start = document.getElementById('txtto10').value;
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "PlacesService.asmx/SearchPlacesNew",
                    data: JSON.stringify({ prefixText: start }),
                    dataType: "json",
                    success: function (data) {
                        response($.map(data.d, function (item) {
                            return {
                                label: item.split('-')[0],
                                val: item.split('-')[1]
                            }
                        }))
                    },
                    error: function (response) {

                    },
                    failure: function (response) {

                    }
                });
            },
            select: function (e, i) {
                $("#hdto10").val(i.item.val);

            },
            minLength: 1
        });
    }
</script>
<script type="text/javascript">
    $(window).load(function () {
        $("#divloading1").fadeOut("slow");


    });
    var prm = Sys.WebForms.PageRequestManager.getInstance();
    prm.add_endRequest(function () {
        outstationapiform();
        function showOnKeyPress() {
            if (document.getElementById("ctl00_SearchControl_desktop_hd").value != "CD") {
                document.getElementById('div_AddPopup').style.display = 'block';
            }
        }

        function hideOnKeyPress() {
            if (document.getElementById("ctl00_SearchControl_desktop_hd").value != "CD") {
                document.getElementById('div_AddPopup').style.display = 'none';
            }
        }


    });
    function EndRequestHandler(sender, args) {

    }

    function load() {

        Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandler);
    }
</script>
