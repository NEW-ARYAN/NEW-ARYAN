<script type="text/javascript">
    function outstationapiform() {
        $("#txtdropaddress").autocomplete({
            source: function (request, response) {
                document.getElementById("ctl00_SearchControl_desktop_hddropoff").value = "";
                var start = document.getElementById('txtdropaddress').value;
                var cityid = document.getElementById('DrpCity').value;
                var hdcitycode = document.getElementById('ctl00_SearchControl_desktop_hdcitycode').value;

                if (cityid > 0) {
                    $("#ctl00_Progress1").show();
                    $.ajax({
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        url: "PlacesService.asmx/GetdropAddresses",
                        data: JSON.stringify({ prefixText: start, eloc: hdcitycode, type: document.getElementById('ctl00_SearchControl_desktop_Drptype').value }),
                        dataType: "json",
                        success: function (data) {
                            response($.map(data.d, function (item) {
                                return {
                                    label: item.split('-')[0],
                                    val: item.split('-')[1]
                                }
                            }))
                            $("#ctl00_Progress1").hide();
                        },
                        error: function (response) {
                            $("#ctl00_Progress1").hide();
                        },
                        failure: function (response) {
                            $("#ctl00_Progress1").hide();
                        }
                    });
                }
                else {
                    document.getElementById("ctl00_SearchControl_desktop_ReqCity").removeAttr("style");
                    document.getElementById("ctl00_SearchControl_desktop_ReqCity").text("Select City");
                }
            },
            select: function (e, i) {
                $("#ctl00_SearchControl_desktop_hddropoff").val(i.item.val);
            },
            minLength: 1
        });

        $("#txtpickupaddress").autocomplete({
            source: function (request, response) {
                document.getElementById("ctl00_SearchControl_desktop_hdpickup").value = "";
                var start = document.getElementById('txtpickupaddress').value;
                var hdcitycode = document.getElementById('ctl00_SearchControl_desktop_hdcitycode').value;
                var cityid = document.getElementById('DrpCity').value;

                if (cityid > 0) {
                    $("#ctl00_Progress1").show();
                    $.ajax({
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        url: "PlacesService.asmx/GetPickAddresses",
                        data: JSON.stringify({ prefixText: start, eloc: hdcitycode, type: document.getElementById('ctl00_SearchControl_desktop_Drptype').value }),
                        dataType: "json",
                        success: function (data) {

                            response($.map(data.d, function (item) {
                                return {
                                    label: item.split('-')[0],
                                    val: item.split('-')[1]
                                }
                            }))
                            $("#ctl00_Progress1").hide();
                        },
                        error: function (response) {
                            $("#ctl00_Progress1").hide();
                        },
                        failure: function (response) {
                            $("#ctl00_Progress1").hide();
                        }
                    });
                }
                else {
                    document.getElementById("ctl00_SearchControl_desktop_ReqCity").removeAttr("style");
                    document.getElementById("ctl00_SearchControl_desktop_ReqCity").text("Select City");
                }

            },
            select: function (e, i) {
                $("#ctl00_SearchControl_desktop_hdpickup").val(i.item.val);
            },
            minLength: 1
        });
        $("#txtOtherCity").autocomplete({
            source: function (request, response) {
                document.getElementById("ctl00_SearchControl_desktop_hdothercity").value = "";
                var start = document.getElementById('txtOtherCity').value;
                $.ajax({
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    url: "PlacesService.asmx/SearchPlacesNew",
                    data: JSON.stringify({ prefixText: start }),
                    dataType: "json",
                    success: function (data) {
                        response($.map(data.d, function (item) {
                            return {
                                label: item.split('-')[0],
                                val: item.split('-')[1]
                            }
                        }))
                    },
                    error: function (response) {

                    },
                    failure: function (response) {

                    }
                });
            },
            select: function (e, i) {
                $("#ctl00_SearchControl_desktop_hdothercity").val(i.item.val);
            },
            minLength: 1
        });
