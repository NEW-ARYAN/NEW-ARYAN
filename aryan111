    <script async src='https://www.googletagmanager.com/gtag/js?id=AW-773187753'></script><script>  window.dataLayer = window.dataLayer || []; function gtag() { dataLayer.push(arguments); } gtag('js', new Date());       gtag('config', 'AW-773187753');</script>
    <script>
        $(document).ready(function () {
            var landingBottomHeight = $(".landingpageDetail").innerHeight();
            $('.landingpageImage').height(landingBottomHeight);
            $('.landingpageImageSD').height(landingBottomHeight);
            clickfunction();
            clickfunction2();
        });
        function clickfunction() {
            /*$(".hideTerms").hide();*/
            if (window.matchMedia('(max-width: 767px)').matches) {
                var list = $('#landingTerms ul li:gt(2)');
                list.hide();
                $('#landingTermMore').click(function () {
                    var txt = list.is(':visible') ? 'Read More' : 'Read Less';
                    $("#landingTermMore").text(txt);
                    list.slideToggle(400);
                    return false;
                });
            } else {
                var list = $('#landingTerms ul');
                list.hide();
                $('#landingTermMore').click(function () {
                    var txt = list.is(':visible') ? '+' : '-';
                    $("#landingTermMore").text(txt);
                    list.slideToggle(400);
                    return false;
                });
            }
        }; function clickfunction2() {
            //$("#uhd ul li:gt(2)").hide();
            if (window.matchMedia('(max-width: 767px)').matches) {
                var list = $('#uhd');
                list.hide();
                $('#uhdmore').click(function () {
                    var txt = list.is(':visible') ? 'Read More' : 'Read Less';
                    $("#uhdmore").text(txt);
                    list.slideToggle(400);
                    return false;
                });
            } else {
                var list = $('#uhd ul');
                list.hide();
                $('#uhdmore').click(function () {
                    var txt = list.is(':visible') ? '+' : '-';
                    $("#uhdmore").text(txt);
                    list.slideToggle(400);
                    return false;
                });
            }
        };
        function onchnagepurpose() {
            if (document.getElementById("DrpPurpose").value == "0" || document.getElementById("DrpPurpose").value == "1") {
                $("#div_bookingno").attr("style", "display:block");
                document.getElementById("ctl00_ContentPlaceHolder1_rqdbookings").enabled = true;
                // ValidatorEnable(document.getElementById('ctl00_ContentPlaceHolder1_rqdbookings'), true);
            }
            else {
                $("#div_bookingno").attr("style", "display:none");
                //ValidatorEnable(document.getElementById('ctl00_ContentPlaceHolder1_rqdbookings'), false);
                document.getElementById("ctl00_ContentPlaceHolder1_rqdbookings").enabled = false;
            }

        }
    </script>
    <script type="text/javascript">
        var classnamecd = $("#lisd").attr("class");
        var classnamesd = $("#licd").attr("class");
        if (classnamecd == "active") {
            $("#cdlowerpart").attr("style", "display:block");
        }
        function cliptoboard(code) {
            var $temp = $("<input>");
            $("body").append($temp);
            $temp.val(code).select();
            document.execCommand("copy");
            $temp.remove();
            $('#thankyoumodal').modal('show');
            $('#couponcodeid').text(code);
            setTimeout(function () {
                $("#thankyoumodal").modal('hide');
            }, 2000);
        };
        function remainopen() {
            $('.modal-backdrop').remove();
            $("#succesmodal").modal("show");
            setTimeout(function () {
                $("#succesmodal").modal('hide');
            }, 2000);
            var landingBottomHeight = $(".landingpageDetail").innerHeight();
            $('.landingpageImage').height(landingBottomHeight);
            $('.landingpageImageSD').height(landingBottomHeight);
            clickfunction();
        }
        function stopen() {
            $('.modal-backdrop').remove();
            $("#querymodal").modal("show");

        }
        var prm = Sys.WebForms.PageRequestManager.getInstance();
        prm.add_endRequest(function () {
            var landingBottomHeight = $(".landingpageDetail").innerHeight();
            $('.landingpageImage').height(landingBottomHeight);
            $('.landingpageImageSD').height(landingBottomHeight);
            function stopen() {
                $('.modal-backdrop').remove();
                $("#querymodal").modal("show");

            }

        });
        function EndRequestHandler(sender, args) {

        }

        function load() {

            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(EndRequestHandler);
        }
    </script>
