<script>

    var femail = $("#hdemail").val();
    var fname = $("#hdfname").val();
    var invite_referrals = window.invite_referrals || {}; (function () {
        invite_referrals.auth = {
            bid_e: 'E871729DF51188E24E2E0D8F8A5D7C53',
            bid: '13917',
            t: '420',
            email: femail, userParams: { 'fname': fname }
        };
        var script = document.createElement('script'); script.async = true;
        script.src = "//cdn.invitereferrals.com/js/invite-referrals-1.0.js";
        var entry = document.getElementsByTagName('script')[0]; entry.parentNode.insertBefore(script, entry);
    })();

    function onlyIntNumbers(evt) {

        var charCode = (evt.which) ? evt.which : evt.keyCode;

        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }

        return true;

    }

</script>

<script>
    window.addEventListener('DOMContentLoaded', function () {
        $("#footertxt").hide();
        $(".footerseeMoreLink").click(function (event) {

            event.preventDefault();
            var footerText = $('.footerSeeClass').text();
            if (footerText == "See More") {
                $(".footerseeMoreLink").html('<span class="footerSeeClass">See Less</span><i class="fa fa-angle-up"></i>');
            } else {
                $(".footerseeMoreLink").html('<span class="footerSeeClass">See More</span><i class="fa fa-angle-down"></i>');
            }
            $("#footertxt").toggle(500);
            $("html, body").animate({ scrollTop: $(document).height() }, 1000);
        });
    });
</script>
