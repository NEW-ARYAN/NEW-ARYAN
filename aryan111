    function onlyIntNumbers(evt) {

        var charCode = (evt.which) ? evt.which : evt.keyCode;

        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }

        return true;

    }

    function onlyAlphabets(evt) {

        var charCode = (evt.which) ? evt.which : evt.keyCode;

        if (charCode == 32 || (charCode >= 65 && charCode <= 90) || (charCode >= 97 && charCode <= 122) || charCode == 8 || charCode == 9) {
            return true;
        }

        return false;

    }
    function onlyDecimalNumbers(evt) {
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if (charCode != 46 && charCode > 31
            && (charCode < 48 || charCode > 57))
            return false;

        return true;
    }


    function getPageName(url) {
        var index = url.lastIndexOf("/") + 1;
        var filenameWithExtension = url.substr(index);
        var filename = filenameWithExtension.split(".")[0]; // <-- added this line
        return filename; // <-- added this line
    }

    function showOnKeyPress() {
        if (document.getElementById("ctl00_SearchControl_desktop_hd").value != "CD") {
            document.getElementById('div_AddPopup').style.display = 'block';
        }
    }

    function hideOnKeyPress() {
        if (document.getElementById("ctl00_SearchControl_desktop_hd").value != "CD") {
            document.getElementById('div_AddPopup').style.display = 'none';
        }
    }





    function hidemultiform(id) {

        var cal = 0;
        for (var value = id; value <= 10; ++value) {
            if (value < 4) {
                var addid = value + 1;
                var mid = value - 1;
                $("#ctl00_txtTo" + value + "").val($("#ctl00_txtTo" + addid + "").val());
                $("#ctl00_hdto" + value + "").val($("#ctl00_hdto" + addid + "").val());
                $("#ctl00_txtFrom" + value + "").val($("#ctl00_txtTo" + mid + "").val());
                $("#ctl00_txtTo" + addid + "").val("");
                $("#ctl00_hdto" + addid + "").val("");
            }
            if (value == 4) {
                var addid = value + 1;
                var mid = value - 1;
                $("#ctl00_txtTo" + value + "").val($("#ctl00_txtto" + addid + "").val());
                $("#ctl00_hdto" + value + "").val($("#ctl00_hdto" + addid + "").val())
                $("#ctl00_txtFrom" + value + "").val($("#ctl00_txtTo" + mid + "").val());
                $("#ctl00_txtto" + addid + "").val("");
                $("#ctl00_hdto" + addid + "").val("");
            }
            if (value > 4) {
                var addid = value + 1;
                var mid = value - 1;
                $("#ctl00_txtto" + value + "").val($("#ctl00_txtto" + addid + "").val());
                $("#ctl00_hdto" + value + "").val($("#ctl00_hdto" + addid + "").val())
                if (mid == 4) {
                    $("#ctl00_txtfrom" + value + "").val($("#ctl00_txtTo" + mid + "").val())
                    $("#ctl00_txtTo" + addid + "").val("");
                    $("#ctl00_hdto" + addid + "").val("");
                }
                else {
                    $("#ctl00_txtfrom" + value + "").val($("#ctl00_txtto" + mid + "").val())
                    $("#ctl00_txtto" + addid + "").val("");
                    $("#ctl00_hdto" + addid + "").val("");
                }
            }
            if ($("#f" + value + "").css('display') != 'none') {
                cal = value;
            }
        }
        $("#f" + cal + "").attr('style', 'display:none');
        if (id <= 10) {
            $("#shw").show();
        }
    }

</script>
<script> 

    function setmultiform() {

        if (document.getElementById('txtTo3').value != "") {
            $("#f3").show();
        }
        if (document.getElementById('txtTo4').value != "") {
            $("#f4").show();
        }
        if (document.getElementById('txtto5').value != "") {
            $("#f5").show();
        }
        if (document.getElementById('txtto6').value != "") {
            $("#f6").show();
        }
        if (document.getElementById('txtto7').value != "") {
            $("#f7").show();
        }
        if (document.getElementById('txtto8').value != "") {
            $("#f8").show();
        }
        if (document.getElementById('txtto9').value != "") {
            $("#f9").show();
        }
        if (document.getElementById('txtto10').value != "") {
            $("#f10").show();
        }

    }

    function showmultiform() {

        var id = "";
        if ($('#f3').css('display') == 'none' || $('#f4').css('display') == 'none' || $('#f5').css('display') == 'none' || $('#f6').css('display') == 'none' || $('#f7').css('display') == 'none' || $('#f8').css('display') == 'none' || $('#f9').css('display') == 'none' || $('#f10').css('display') == 'none') {
            if ($('#f3').css('display') == 'none') {
                id = "f3";

            }
            else if ($('#f3').css('display') == 'block' && $('#f4').css('display') == 'none') {
                id = "f4";
                //                    $("#shw").hide();
                //$("#hide").show();
            }
            else if ($('#f4').css('display') == 'block' && $('#f5').css('display') == 'none') {
                id = "f5";

            }
            else if ($('#f5').css('display') == 'block' && $('#f6').css('display') == 'none') {
                id = "f6";

            }
            else if ($('#f6').css('display') == 'block' && $('#f7').css('display') == 'none') {
                id = "f7";

            }
            else if ($('#f7').css('display') == 'block' && $('#f8').css('display') == 'none') {
                id = "f8";

            }
            else if ($('#f8').css('display') == 'block' && $('#f9').css('display') == 'none') {
                id = "f9";

            }
            else if ($('#f9').css('display') == 'block' && $('#f10').css('display') == 'none') {
                id = "f10";
                $("#shw").hide();
            }
            var count = $('.multiCityForm .form-group').filter(function () {
                return $(this).css('display') == 'none';
            }).length;


            if (count == 1) { $("#shw").hide(); }
            $("#" + id + "").show();
        }
    }</script>
<script type="text/javascript">
    function onlyIntNumbers(evt) {

        var charCode = (evt.which) ? evt.which : evt.keyCode;

        if (charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }

        return true;

    }

    function onlyAlphabets(evt) {

        var charCode = (evt.which) ? evt.which : evt.keyCode;

        if (charCode == 32 || (charCode >= 65 && charCode <= 90) || (charCode >= 97 && charCode <= 122) || charCode == 8 || charCode == 9) {
            return true;
        }

        return false;

    }
    function onlyDecimalNumbers(evt) {
        var charCode = (evt.which) ? evt.which : evt.keyCode;
        if (charCode != 46 && charCode > 31
            && (charCode < 48 || charCode > 57))
            return false;

        return true;
    }

